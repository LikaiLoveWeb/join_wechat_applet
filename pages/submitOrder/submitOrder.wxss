/* 订单列表 css */

.shopInfo {
  width: 100%;
  height: 88rpx;
  background-color: #fff;
  border-bottom: 2rpx solid #e6e6e6;
  position: relative;
}

.shopImg {
  width: 34rpx;
  height: 34rpx;
  position: absolute;
  left: 30rpx;
  top: 28rpx;
}

.shopName {
  position: absolute;
  top: 30rpx;
  left: 84rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  display: -webkit-box;
  width: 636rpx;
  display: inline-block;
  font-size: 30rpx;
  color: #333;
}

.commodityListContent {
  width: 690rpx;
  padding: 10rpx 30rpx 0 30rpx;
  background-color: #fff;
  display: flex;
  font-size: 24rpx;
  color: #333;
}

.comContentImg {
  width: 180rpx;
  height: 180rpx;
}

.composter {
  width: 180rpx;
  height: 180rpx;
}

.comContenInfo {
  width: 500rpx;
  padding-left: 10rpx;
}

.comDesc {
  display: flex;
  justify-content: space-between;
  margin-top: 2rpx;
  line-height: 38rpx;
}

.comTitle {
  width: 328rpx;
  height: 74rpx;
  overflow: hidden;
  text-overflow: ellipsis;
}

.comParameter {
  line-height: 38rpx;
  margin-top: 4rpx;
  color: #999;
}

.comPrice {
  line-height: 38rpx;
  margin-top: 10rpx;
  display: flex;
  justify-content: space-between;
}

.comNumb {
  color: #999;
}

.price {
  font-weight: 600;
}

.freightInfo {
  height: 88rpx;
  width: 690rpx;
  margin: 10rpx 0 0 30rpx;
  border-top: 2rpx solid #e6e6e6;
  border-bottom: 2rpx solid #e6e6e6;
}

.freightInput_left {
  float: left;
  margin-top: 12rpx;
  position: relative;
}

.freightInput {
  display: inline-block;
  width: 202rpx;
  height: 56rpx;
  border: 2rpx solid #6e08a8;
  outline: none;
  padding-left: 14rpx;
  font-size: 24rpx;
}

.freight {
  width: 30rpx;
  height: 26rpx;
  position: absolute;
  right: 24rpx;
  top: 18rpx;
}

.freightDesc_right {
  float: left;
  margin-left: 30rpx;
  margin-top: 20rpx;
}

.freightDesc {
  font-size: 26rpx;
  line-height: 36rpx;
  color: #333;
}

.inquiryFreight {
  font-size: 24rpx;
  line-height: 36rpx;
  color: #999;
}

.commodityRemark {
  width: 100%;
  height: 114rpx;
  /* padding: 0 0 0 0rpx; */
}

.LeavingMessage {
  display: inline-block;
  width: 668rpx;
  height: 70rpx;
  border-radius: 6rpx;
  outline: none;
  padding-left: 22rpx;
  font-size: 24rpx;
  color: #333;
  background-color: #f5f5f5;
  margin-top: 20rpx;
  margin-left: 30rpx;
}

.commodityTotal {
  height: 132rpx;
  width: 100%;
  padding: 0 -30rpx 0 -30rpx;
  border-top: 2rpx solid #e6e6e6;
  border-bottom: 20rpx solid #f5f5f5;
  color: #333;
  font-size: 28rpx;
  display: flex;
  align-items: center;
  justify-content: flex-end;
}

.commodityNum {
}

.verticalPartition {
  width: 1rpx;
  height: 40rpx;
  border-left: 2rpx solid #e6e6e6;
}

.commodityTest {
}

.commodityPrice {
  margin-right: 30rpx;
  font-weight: 600;
  color: #6e08a8;
}

.division2 {
  width: 100%;
  height: 10rpx;
  background-color: #f5f5f5;
  border-bottom: 2rpx solid #e6e6e6;
}

