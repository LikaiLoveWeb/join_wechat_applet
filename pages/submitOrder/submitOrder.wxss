/* 地址部分 css  */

.indexTopClass {
  width: 100%;
  height: 4rpx;
  position: relative;
}

.indexTopImage {
  width: 100%;
  height: 4rpx;
  position: absolute;
}

/* 有收货地址，并且地址字段长度大于18时 css*/

.contact_box {
  width: 100%;
  height: 168rpx;
  overflow: hidden;
  position: relative;
  background-color: #fff;
}

.left_box {
  float: left;
  margin-left: 30rpx;
  margin-top: 64rpx;
}

.left_icon {
  width: 26rpx;
  height: 43rpx;
}

.content_box {
  float: left;
  height: 96rpx;
  width: 570rpx;
  margin-left: 20rpx;
  margin-top: 30rpx;
  position: relative;
}

.content_top {
  font-size: 28rpx;
  color: #333;
}

.content_buttom {
  font-size: 24rpx;
  line-height: 36rpx;
  color: #666;
  position: absolute;
  top: 43rpx;
}

.right_box {
  float: right;
  margin-right: 30rpx;
  margin-top: 64rpx;
}

.right_icon {
  width: 16rpx;
  height: 28rpx;
}

