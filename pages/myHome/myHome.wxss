/* pages/end/end.wxss */
.myBox{
  width:100%;
  height:1110rpx;
  background:#f5f5f5;
}
/* 我的头部 */
.myTop{
  height:200rpx;
  background:#ffffff;
}
.blockImg{
  margin-left:10rpx;
  margin-top:10rpx;
}
.mytopLeft{
   float:left;
}
.mytopLeft .myImg{
  width:120rpx;
  height:120rpx;
  margin:30rpx;
  border-radius:120rpx;
}
.mytopRight{
  float:left;
  margin-top:45rpx;
  position:relative;
}
.userinfo-nickname{
  font-size:32rpx;
  color:#010101;
}
.Myaddress{
  font-size:24rpx;
  color:#999999;
  margin-top:24rpx;
}
/* 我的内容 */
.myController{
  height:auto;
  background:#ffffff;
  margin-top:20rpx;
  
}
.info_list{
  width:690rpx;
  margin:0 auto;
}
.weui_cell{
    position: relative;
    display: flex;
    /* padding: 15px; */
    height:88rpx;
    line-height:88rpx;
    -webkit-box-align: center;
    -ms-flex-align: center;
    align-items: center;
    border-bottom: 1px solid #e6e6e6;
}
.weui_cell:nth-child(4) {
  border-bottom:none;
}
.weui_cell_Img{
  display: inline-block;
  width: 20px;
  margin-right: 5px;
}
.weui_cell_Img image{
  width: 100%;
  height: 20px;
  vertical-align: -2px;
}

.weui_cell_Text{
  display: inline-block;
}
.weuiText{
  font-size:30rpx;
  color: #333;
  margin-left:24rpx;
}
.badge{
  position: absolute;
  top: 18px;
  right: 40px;
  width: 15px;
  height: 15px;
  line-height: 15px;
  background: #ff0000;
  color: #fff;
  border-radius: 50%;
  text-align: center;
  font-size: 8px;
}
.with_arrow{
  position: absolute;
  /* top: 18px; */
  right: 5px;
  display: inline-block;
  width: 20px;
}
.with_arrow .with_arrowImg{
  width: 16rpx;
  height: 28rpx;
  vertical-align: -2px;
}


